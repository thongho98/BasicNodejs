extends ../layouts/common
block content
  h1 Users
  a(href="/users/create") News
  form(action="/users/search", method="GET")
    input(type="text", name="q")
    button(class="btn btn-primary") Search
  ul
    each user in users
      li 
        span= user.name
        span &nbsp;
        a(href='/users/'+user.id) View